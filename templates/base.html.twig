<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            <link rel="stylesheet" href="{{ asset('build/css/app.css')}}">
            <!-- Latest compiled and minified CSS -->
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
            <!-- add external css -->
            <link rel="stylesheet" href="{{ asset('build/css/styles.css') }}"/>
            <!-- Optional theme -->
            <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap-theme.min.css" integrity="sha384-rHyoN1iRsVXV4nD0JutlnGaslCJuC7uwjduW9SVrLvRYooPp2bWYgmgJQIXwl/Sp" crossorigin="anonymous">
        {% endblock %}
    </head>
    <body>
           <header>
	<section>
		<img src={{ asset('build/img/logo3.png')}}/>
		<ul>
			<li><a href="#"><img src={{ asset('build/img/female.png')}}><br/>{{ 'Name of the user'| trans }}</a></li>
			<li><a href="#"><img src={{ asset('build/img/header/add.png')}}/><br/>{{ 'Add news' | trans }}</a></li>
			<li><a href="{{path("homepage")}}"><img src={{ asset('build/img/header/back-arrow.png')}}/><br/>Return</a></li>
		</ul>
	</section>
</header>

        {% block body %}{% endblock %}
        

        {% block javascripts %} 
 <script src="https://code.jquery.com/jquery-3.1.1.min.js"</script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
       <script src="{{asset('build/js/app.js')}}"></script>   {% endblock %}


       
         <footer> 
	<ul class="lang">
		<li><a <a href="{{ path(app.request.get('_route'), app.request.get('_route_params')|merge({'_locale': 'fr'})) }}"><img src={{ asset('build/img/lang/france.png')}}/></a></li>
		<li><a href="{{ path(app.request.get('_route'), app.request.get('_route_params')|merge({'_locale': 'bg'})) }}"><img src={{ asset('build/img/lang/bulgarie.png')}}/></a></li>
		<li><a href="{{ path(app.request.get('_route'), app.request.get('_route_params')|merge({'_locale': 'en'})) }}"><img src={{ asset('build/img/lang/united-kingdom.png')}}/></a></li>
	</ul>

	<ul class="links">
		<li><a href="{{ path('Notice') }}">   {{ 'Legal notice'|trans }}</a></li>             
		<li><a href="{{ path('About') }}"> {{'About us'| trans }}</a></li>
		<li><a href="{{ path('Termsofuse') }}">{{'Terms of use' |trans }}</a></li>
		<li><a href="{{ path('Questions') }}">{{'Questions'|trans }}</a></li>
	</ul>
	
	<ul class="sns">
		<li><a href="#"><img src={{ asset('build/img/male.png')}}/></a></li>
		<li><a href=""><img src={{ asset('build/img/sns/instagram-logo.png')}}/></a></li>
		<li><a href="#"><img src={{ asset('build/img/sns/twitter-logo.png')}}/></a></li>
	</ul>
</footer>
     

        <div class = "container">
            {%  include('flash-messages.html.twig') %}
            {% if app.user %}
                <a href="{{ path('logout') }}">LOGOUT</a>
            {% endif %}
            {% block body %}

            {% endblock %}

        </div>

        {% block javascripts %}
            <script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
            <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
            <script src="{{asset('build/js/app.js')}}"></script>
        {% endblock %}
    </body>
  
</html>
